export(PackedScene) var coin :PackedScene
export(NodePath) var spawner_coordinate
const MAX_COINS :int = 10
const BATCH_SIZE = 10
const key :String = \"coins\"
var list :Array
var last :Node
var current :Node

func _ready():
	Pool.create_population(MAX_COINS, key, coin)
	spawner_coordinate = get_node_or_null(spawner_coordinate)
	add_in_line()

func sprayed():
	var position = spawner_coordinate.random_position()
	current.transform.origin = last.transform.origin + (GameParam.speed / 10) * Vector3.FORWARD
	current.show()

func add_in_line():
	last = current
	
	current = Pool.release(key)
	if current == null:
		current = Pool.objetct_factory(key)
	if current.get_parent() == null:
		add_child(current)
	current.initial_state()
	last = current
	sprayed()
"

